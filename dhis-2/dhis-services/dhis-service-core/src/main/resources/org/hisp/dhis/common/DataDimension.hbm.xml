<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.common.DataDimension" table="datadimension">
  
    <id name="id" column="datadimensionid">
      <generator class="native" />
    </id>

    <many-to-one name="indicator" class="org.hisp.dhis.indicator.Indicator" 
        column="indicatorid" foreign-key="fk_datadimension_indicatorid" />
  
    <many-to-one name="dataElement" class="org.hisp.dhis.dataelement.DataElement" 
        column="dataelementid" foreign-key="fk_datadimension_dataelementid" />
  
    <many-to-one name="dataSet" class="org.hisp.dhis.dataset.DataSet"
        column="datasetid" foreign-key="fk_datadimension_datasetid" />
  
    <many-to-one name="programIndicator" class="org.hisp.dhis.program.ProgramIndicator" 
        column="programindicatorid" foreign-key="fk_datadimension_programindicatorid" />
  
    <many-to-one name="trackedEntityAttribute" class="org.hisp.dhis.trackedentityattribute.TrackedEntityAttribute" 
        column="trackedentityattributeid" foreign-key="fk_datadimension_trackedentityattributeid" />
  
  </class>
</hibernate-mapping>
