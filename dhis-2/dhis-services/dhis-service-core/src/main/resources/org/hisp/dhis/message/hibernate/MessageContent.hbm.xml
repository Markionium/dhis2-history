<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.message.MessageContent" table="messagecontent">

    <cache usage="read-write" />

    <id name="id" column="messagecontentid">
      <generator class="native" />
    </id>
	
	<property name="key"/>
	<property name="text"/>
	
    <many-to-one name="sender" class="org.hisp.dhis.user.User" column="userid" 
	  foreign-key="fk_messagecontent_userid" />
	  
  </class>
</hibernate-mapping>
	
