<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.hisp.dhis.activityplan.ActivityPlan" table="activityplan">
		
		<id name="id" column="activityplanid">
			<generator class="native"/>
		</id>
		
		<many-to-one name="hew" class="org.hisp.dhis.user.User"
			column="userinfoid"/>
		
		<many-to-one name="organisationUnit"
			class="org.hisp.dhis.organisationunit.OrganisationUnit"
			column="organisationunitid"/>
		
		<many-to-one name="period" class="org.hisp.dhis.period.Period"
			column="periodid"/>
		
		<set name="encounters" table="activityplan_encounters">
			<key column="activityplanid"/>
			<many-to-many class="org.hisp.dhis.encounter.Encounter"
				column="encounterid" foreign-key="fk_activityplan_encounteridd"/>
		</set>
		
	</class>
</hibernate-mapping>