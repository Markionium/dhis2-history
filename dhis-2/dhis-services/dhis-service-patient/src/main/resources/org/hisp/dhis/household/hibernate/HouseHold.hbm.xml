<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.hisp.dhis.household.HouseHold" table="household">
		
		<id name="id" column="householdid">
			<generator class="native"/>
		</id>
		
		<property name="houseNumber" length="31"/>
		
		<property name="visitingOrder" column="visitingorder"/>
		
		<property name="address" column="address"/>
		
		<property name="landMark" column="landmark"/>
		
		<many-to-one name="organisationUnit" class="org.hisp.dhis.organisationunit.OrganisationUnit"
			column="organisationunitid" not-null="true"
			foreign-key="fk_household_organisationunitid"/>       
		
	</class>
</hibernate-mapping>