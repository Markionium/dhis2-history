<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.hisp.dhis.patient.PatientAddress" table="patientaddress">
		
		<id name="id" column="patientaddressid">
			<generator class="native"/>
		</id>
		
		<many-to-one name="patient" class="org.hisp.dhis.patient.Patient" column="patientid"/>
		
		<many-to-one name="house" class="org.hisp.dhis.organisationunit.OrganisationUnit" column="organisationunitid"/>
		
		<property name="address1" column="address1"	length="50"/>
		
		<property name="address2" column="address2"	length="50"/>
		
		<property name="cityVillage" column="city_village" length="50"/>
		
		<property name="stateProvince" column="state_province" length="50"/>
		
		<property name="country" column="country" length="50"/>
		
		<property name="postalCode" column="postal_code" length="50"/>
		
		<property name="landMark" length="50"/>
		
		<property name="preferred" column="preferred" not-null="true"/>
		
	</class>
</hibernate-mapping>